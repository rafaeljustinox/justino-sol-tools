{
  "manifest_version": 2,

  "name": "Justino SOL Tools",
  "description": "Turbinando o sistema online da PUC Goi√°s ",
  "version": "1.0",
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_120.png"
  },
  "browser_action": {
    "default_icon": "images/icon_120.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/js/jquery-3.4.1.min.js", "script.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage"
  ],
  "options_ui": {
    "page": "options.html"
  }
}